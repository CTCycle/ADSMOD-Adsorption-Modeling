{
  "database": {
    "embedded_database": false,
    "engine": "postgres",
    "host": "localhost",
    "port": 5432,
    "database_name": "ADSMOD",
    "username": "postgres",
    "password": "admin",
    "ssl": false,
    "ssl_ca": null,
    "connect_timeout": 30,
    "insert_batch_size": 20000,    
    "browse_page_size": 1000
  },
  "datasets": {
    "allowed_extensions": [
      ".csv",
      ".xls",
      ".xlsx"
    ],
    "column_detection_cutoff": 0.6
  },
  "nist": {
    "parallel_tasks": 20,
    "pubchem_parallel_tasks": 1
  },
  "fitting": {
    "default_max_iterations": 1000,
    "max_iterations_upper_bound": 1000000000,
    "default_parameter_initial": 1.0,
    "default_parameter_min": 0.0,
    "default_parameter_max": 100.0,
    "preview_row_limit": 20,
    "best_model_metric": "AICc"
  },
  "training": {
    "use_jit": false,
    "jit_backend": "inductor",
    "use_mixed_precision": false,
    "dataloader_workers": 0,
    "prefetch_factor": 1,
    "pin_memory": true,
    "persistent_workers": false,
    "polling_interval": 1.0,
    "plot_update_batch_interval": 10
  }
}
